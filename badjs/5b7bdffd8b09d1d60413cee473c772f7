function openbox(id){
	mobddmenu = document.getElementById('box').style.display;
	mobddsearch = document.getElementById('mob-dd-search').style.display;
	if (mobddmenu=='none' && mobddsearch=='none'){
		document.getElementById(id).style.display='block';
    }
	else if (mobddmenu=='block'){
		document.getElementById(id).style.display='block';
		document.getElementById('box').style.display='none';
	}
	else if (mobddsearch=='block'){
		document.getElementById(id).style.display='block';
		document.getElementById('mob-dd-search').style.display='none';
	}
	else{
		document.getElementById('box').style.display='none';
		document.getElementById('mob-dd-search').style.display='none';
	}
}
		

$("#box .drop_down > a, #box .drop_down2.m_i > a").click(function(e)
{      
  var $sm0 = $(this).siblings(".submenu");
  var $sm1 = $(this).siblings(".submenu2");

  if ($sm0.length && $sm0.not(":visible").length) { $sm0.slideDown(200); e.preventDefault(); return false;}
  else if ($sm1.length && $sm1.not(":visible").length) { $sm1.slideDown(200); e.preventDefault(); return false;}
  
  return true;
});


