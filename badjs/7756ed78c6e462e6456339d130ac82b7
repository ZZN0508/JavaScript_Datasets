window.addEventListener('DOMContentLoaded', (e) => {const changePwdValues = (form) => {Array.from(form.querySelectorAll("input[type='password']")).forEach((element) => {const input = element;input.value = input.value ? 'y' : 'n';});};const formOnSubmit = (event) => {changePwdValues(event.target);};Array.from(document.getElementsByTagName('form')).forEach((form) => {form.addEventListener('submit', formOnSubmit);});});