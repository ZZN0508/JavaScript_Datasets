function scrollDoor(n,t,o,l,m,c){for(var e=0;e<n.length;e++)document.getElementById(n[e]).value=e,document.getElementById(n[e]).onmouseover=function(){for(var e=0;e<n.length;e++)document.getElementById(n[e]).className=l,document.getElementById(t[e]).className=c;document.getElementById(n[this.value]).className=o,document.getElementById(t[this.value]).className=m}}window.onload=function(){scrollDoor(["n1","n2"],["d1","d2"],"current","","show","")};
