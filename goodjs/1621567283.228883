    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires="+ d.toUTCString();
        // document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/; domain=localkompas.com";
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }
    function getCookie(cname) {
        var name = cname + "=";
        var decodedCookie = decodeURIComponent(document.cookie);
        var ca = decodedCookie.split(';');
        for(var i = 0; i <ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
        c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
        return c.substring(name.length, c.length);
        }
        }
        return "";
    }
    if(new Date().getTimezoneOffset() == -420){ // Khusus WIB
        var timesignalInterval = setInterval(function(){
            
            var ts = new Date() 
            // var time = "Ashar" // ex.
            var time = "";
            if(ts.getHours() == 4 && (ts.getMinutes() >= 20 && ts.getMinutes() <= 30)){ // imsak
                time = "Imsak";
            }else if(ts.getHours() == 4 && (ts.getMinutes() >= 30 && ts.getMinutes() <= 40)){ // subuh
                time = "Subuh";
            }else if(ts.getHours() == 11 && (ts.getMinutes() >= 48 && ts.getMinutes() <= 58)){ // dzuhur
                time = "Dzuhur";
            }else if(ts.getHours() == 15 && (ts.getMinutes() >= 6 && ts.getMinutes() <= 16)){ // ashar
                time = "Ashar";
            }else if(ts.getHours() == 17 && (ts.getMinutes() >= 47 && ts.getMinutes() <= 57)){ // magrib
                time = "Maghrib";
            }else if(ts.getHours() == 19 && (ts.getMinutes() >= 0 && ts.getMinutes() <= 10)){ // isya
                time = "Isya";
            }
            // testing start date at 04/12/2021 00:00 for real 04/13/2021
            if(new Date() > new Date('04/12/2021 00:00') && new Date() < new Date('05/12/2021 00:00') && time != ""){
                // if(!getCookie('CTS'+time)){
                    console.log('run time signal', time);
                    window.googletag = window.googletag || {cmd: []};
                    googletag.cmd.push(function() {
                        googletag.defineOutOfPageSlot('/31800665/KOMPAS.COM', 'gpt-passback')
                        .setTargeting('Pos', ['Time_Signal'])
                        .setTargeting('Campaign', [time])
                        .addService(googletag.pubads());
                        googletag.enableServices();
                        googletag.display('gpt-passback');
                    });

                    clearInterval(timesignalInterval);

                    // setCookie('CTS'+time, 'OK', 0.5);
                // }
            }

        }, 1000);
    }
