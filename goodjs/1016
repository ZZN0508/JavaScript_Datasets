define(["OK/HookModel"],function(e){var a;return{activate:function(o){var t=o.getAttribute("data-loader-id"),t=e.extractHookId(t);(a=e.hookActivated(t)).then(function(){setTimeout(function(){var t=document.createEvent("MouseEvents"),e=o.parentNode.getElementsByClassName("js-show-more")[0];t.initEvent("click",!1,!0),e.dispatchEvent(t)})})},deactivate:function(t){t=t.getAttribute("data-loader-id"),t=e.extractHookId(t);a&&e.removeObserver(a,t)}}});
